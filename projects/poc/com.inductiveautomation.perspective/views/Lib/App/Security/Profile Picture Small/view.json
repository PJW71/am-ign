                  "enabled": false,
                  "transforms": [
                    {
                      "expression": "if({value}\u003dnull||len({value})\u003d0,\"Sign In\",{value})",
                      "type": "expression"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "style": {
                "cursor": "pointer"
              },
              "text": "tamer.ahmed-salem@northvolt.com",
              "textStyle": {
                "fontFamily": "Poppins",
                "fontSize": 16,
                "fontWeight": "bold"
              }
            },
            "type": "ia.display.label"
          },
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tif self.session.props.auth.authenticated:\n\t\tsystem.perspective.logout()\n\telse:\n\t\tsystem.perspective.login()\t"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "hasDelegate": true,
              "name": "Fullname",
              "tooltip": {
                "enabled": true,
                "location": "bottom",
                "style": {
                  "textAlign": "left",
                  "whiteSpace": "pre-wrap"
                },
                "tail": false,
                "text": "First Name \u0026 Last Name",
                "width": "upper("
              }
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "position.display": {
                "binding": {
                  "config": {
                    "path": "session.props.auth.authenticated"
                  },
                  "type": "property"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
                    "path": "session.props.auth.user.firstName"
                  },
                  "transforms": [
                    {
                      "expression": "{value}",
                      "type": "expression"
                    }
                  ],
                  "type": "property"
                }
              }
            },
            "props": {
              "textStyle": {
                "fontFamily": "sofia pro",
                "fontSize": 16
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Name"
        },
        "position": {
          "grow": 1
        },
        "props": {
          "alignItems": "center",
          "direction": "column",
          "style": {
            "marginLeft": "2px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": "\tif self.session.props.auth.authenticated:\n\t\tsystem.perspective.logout()\n\telse:\n\t\tsystem.perspective.login()\t\t\t"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Icon",
          "tooltip": {
            "enabled": true,
            "location": "bottom",
            "tail": false
          }
        },
        "position": {
          "basis": "24px",
          "shrink": 0
        },
        "propConfig": {
          "meta.tooltip.text": {
            "binding": {
              "config": {
                "expression": "if({session.props.auth.authenticated}, \r\n\"Click to Sign Out\",\r\n\"Click to Sign In\")"
              },
              "type": "expr"
