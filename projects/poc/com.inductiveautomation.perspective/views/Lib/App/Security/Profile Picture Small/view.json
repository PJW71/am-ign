{
  "custom": {
    "key": "value"
  },
  "params": {
    "login": true
  },
  "propConfig": {
    "custom.key": {
      "persistent": true
    },
    "params.feature": {
      "paramDirection": "input"
    },
    "params.login": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.tier1": {
      "paramDirection": "input"
    },
    "params.tier2": {
      "paramDirection": "input"
    }
  },
  "props": {
    "defaultSize": {
      "height": 64,
      "width": 246
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Icon_0"
        },
        "position": {
          "basis": "42px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "!{session.props.auth.authenticated}"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "path": "sp1/unkown-user",
          "style": {
            "margin": "0 5px 0 0"
          }
        },
        "type": "ia.display.icon"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": ""
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Image",
          "tooltip": {
            "delay": 1000,
            "enabled": true,
            "location": "bottom",
            "tail": false,
            "text": "Click to Change Your Picture"
          }
        },
        "position": {
          "basis": "54px",
          "display": false,
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "session.props.auth.authenticated"
              },
              "enabled": false,
              "type": "property"
            }
          },
          "props.source": {
            "binding": {
              "config": {
                "struct": {
                  "gatewayAddress": "{session.props.gateway.address}",
                  "username": "{session.props.auth.user.userName}"
                },
                "waitOnAll": true
              },
              "overlayOptOut": true,
              "transforms": [
                {
                  "code": "\timport base64\n\n\tuserName \u003d self.session.props.auth.user.userName\n\tnoImage \u003d \u0027\u0027\u0027data:imagef;base64,\u003d\u0027\u0027\u0027\n\t\n\tresult \u003d system.db.runPrepQuery(\"SELECT image from users WHERE username \u003d ?\", [userName], \"Users\")\n\tif len(result) \u003d\u003d 0: # There is no image save for this user in DB\n\t\treturn noImage\n\telse:\n\t\tbytes \u003d result[0][0]\n\t\tif bytes \u003d\u003d None: # There is no image save for this user in DB\n\t\t\treturn noImage\n\t\telse:\t\n\t\t\tencoded \u003d base64.b64encode(bytes)\n\t\t\treturn \"data:imagef;base64,%s\"%encoded",
                  "type": "script"
                }
              ],
              "type": "expr-struct"
            }
          }
        },
        "props": {
          "fit": {
            "height": 30,
            "mode": "fill",
            "width": 30
          },
          "style": {
            "border": "2px #FAFCFF solid",
            "border-radius": "50%"
          },
          "tint": {
            "color": "#FAFCFF"
          }
        },
        "type": "ia.display.image"
      },
      {
        "events": {
          "component": {
            "onFileReceived": {
              "config": {
                "script": "\tusername \u003d self.session.props.auth.user.userName\n\tbytes \u003d event.file.getBytes()\n\t\n\tUser.updatePicture(username, bytes)\t\t\n\n\tself.getSibling(\"Image\").refreshBinding(\"props.source\")\t\t\t\t\t\t   \n\t\t\t\t\t\t\t   "
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "FileUpload",
          "tooltip": {
            "delay": 1000,
            "enabled": true,
            "text": "Click to Upload Your Picture"
          }
        },
        "position": {
          "basis": "54px",
          "display": false,
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "session.props.auth.authenticated"
              },
              "enabled": false,
              "type": "property"
            }
          }
        },
        "props": {
          "fileSizeLimit": 0.1,
          "fileUploadIcon": {
            "path": "",
            "style": {
              "classes": ""
            }
          },
          "maxUploads": 1,
          "style": {
            "classes": "",
            "cursor": "pointer",
            "margin": "0  10px 0 -54px"
          },
          "supportedFileTypes": [
            "jpg"
          ]
        },
        "type": "ia.input.fileupload"
      },
      {
        "events": {
          "dom": {
            "onClick": {
              "config": {
                "script": ""
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "Image_0",
          "tooltip": {
            "delay": 1000,
            "enabled": true,
            "location": "bottom",
            "tail": false,
            "text": "Click to Change Your Picture"
          }
        },
        "position": {
          "basis": "40px",
          "shrink": 0
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "path": "session.props.auth.authenticated"
              },
              "type": "property"
            }
          },
          "props.source": {
            "binding": {
              "config": {
                "struct": {
                  "access_token": "{session.props.auth.idpAttributes.tokenEndpointResponse.access_token}",
                  "username": "{session.props.auth.user.email}"
                },
                "waitOnAll": true
              },
              "overlayOptOut": true,
              "transforms": [
                {
                  "code": "\timport base64\n\tgraph_api_url \u003d \u0027https://graph.microsoft.com/v1.0/users/\u0027 + value.username + \u0027/photo/$value\u0027\n\theaders \u003d {\u0027Authorization\u0027: \u0027Bearer \u0027 + value.access_token}\n\tclient \u003d system.net.httpClient()\n\tresponse \u003d client.get(graph_api_url, headers\u003dheaders)\n\tif response.statusCode\u003d\u003d200:\n\t\tcontent_type \u003d response.headers [\u0027Content-Type\u0027]\n\t\tif \u0027image/jpeg\u0027 in content_type:\n\t\t\tencoded \u003d base64.b64encode(response.body)\n\t\t\treturn \"data:imagef;base64,%s\"%encoded\t\n\n",
                  "type": "script"
                }
              ],
              "type": "expr-struct"
            }
          }
        },
        "props": {
          "fit": {
            "height": 25,
            "mode": "fill",
            "width": 25
          },
          "style": {
            "border": "2px #FAFCFF solid",
            "border-radius": "50%"
          },
          "tint": {
            "color": "#FAFCFF"
          }
        },
        "type": "ia.display.image"
      },
      {
        "children": [
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tif self.session.props.auth.authenticated:\n\t\tsystem.perspective.logout()\n\telse:\n\t\tsystem.perspective.login()\t"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "hasDelegate": true,
              "name": "username",
              "tooltip": {
                "enabled": true,
                "location": "bottom",
                "style": {
                  "textAlign": "left",
                  "whiteSpace": "pre-wrap"
                },
                "tail": false
              }
            },
            "position": {
              "display": false,
              "grow": 1
            },
            "propConfig": {
              "meta.tooltip.text": {
                "binding": {
                  "config": {
                    "expression": "if({session.props.auth.authenticated}, \r\n\"Click to Sign Out\",\r\n\"Click to Sign In\")"
                  },
                  "type": "expr"
                }
              },
              "props.text": {
                "binding": {
                  "config": {
