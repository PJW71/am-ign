{
  "custom": {},
  "params": {
    "errorMessage": [],
    "type": "create"
  },
  "propConfig": {
    "params.errorMessage": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.type": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 230,
      "width": 300
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Header"
        },
        "position": {
          "basis": "30px",
          "shrink": 0
        },
        "propConfig": {
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.type"
              },
              "transforms": [
                {
                  "fallback": "Error Saving Roster",
                  "inputType": "scalar",
                  "mappings": [
                    {
                      "input": "create",
                      "output": "Error Creating Roster"
                    }
                  ],
                  "outputType": "scalar",
                  "type": "map"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "backgroundColor": "#790000",
            "classes": "RosterManagement/Header",
            "fontSize": "15px",
            "marginBottom": "10px",
            "textTransform": "capitalize"
          }
        },
        "type": "ia.display.label"
      },
      {
        "meta": {
          "name": "Markdown"
        },
        "position": {
          "basis": "200px"
        },
        "propConfig": {
          "props.source": {
            "binding": {
              "config": {
                "path": "view.params.errorMessage"
              },
              "transforms": [
                {
                  "code": "\terrorMessages \u003d value\n\tfor errorMessage in errorMessages:\n\t\tstring \u003d \"\\n\"\n\t\tstring +\u003d (\"+ \" + errorMessage[\"errorMessage\"] + \"\\n\")\n\t\t\n\treturn string",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "ScheduleManagement/InputFont",
            "fontWeight": "normal",
            "marginLeft": "-8px",
            "marginRight": "8px",
            "overflowWrap": "normal",
            "textAlign": "left",
            "textTransform": "capitalize"
          }
        },
        "type": "ia.display.markdown"
      },
      {
        "events": {
          "component": {
            "onActionPerformed": {
              "config": {
                "draggable": true,
                "id": "RosterManagement_SaveError",
                "modal": false,
                "overlayDismiss": false,
                "resizable": true,
                "showCloseIcon": true,
                "type": "close",
                "viewPath": "UserManagement/Views/Popup_SaveError"
              },
              "scope": "C",
              "type": "popup"
            }
          }
        },
        "meta": {
          "name": "Button"
        },
        "position": {
          "basis": "35px",
          "shrink": 0
        },
        "props": {
          "style": {
            "fontSize": "14px",
            "marginBottom": "10px",
            "marginLeft": "80px",
            "marginRight": "80px",
            "marginTop": "10px"
          },
          "text": "Close"
        },
        "type": "ia.input.button"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "fontWeight": "bold",
        "textAlign": "center",
        "textTransform": "uppercase"
      }
    },
    "type": "ia.container.flex"
  }
}