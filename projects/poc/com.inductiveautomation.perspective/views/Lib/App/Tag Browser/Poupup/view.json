{
  "custom": {},
  "params": {
    "tagPath": null
  },
  "propConfig": {
    "custom.tagPath": {
      "binding": {
        "config": {
          "path": "view.params.tagPath"
        },
        "type": "property"
      }
    },
    "params.tagPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 338,
      "width": 306
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Providers"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "custom.icon": {
            "binding": {
              "config": {
                "path": "this.props.params.value"
              },
              "transforms": [
                {
                  "code": "\tfor item in self.props.params.options:\n\t\tif item[\u0027value\u0027] \u003d\u003d value:\n\t\t\treturn item[\u0027icon\u0027]\n",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.params.options": {
            "binding": {
              "config": {
                "path": ""
              },
              "transforms": [
                {
                  "code": "\n\tproviders \u003d Util.tagProviders()\n\toptions \u003d []\n\tfor provider in providers:\n\t\toptions.append({\"label\": provider, \"value\": provider})\n\treturn options\n\n",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          },
          "props.params.value": {
            "binding": {
              "config": {
                "path": "view.params.tagPath"
              },
              "overlayOptOut": true,
              "transforms": [
                {
                  "code": "\treturn value.split(\"]\")[0] + \"]\"",
                  "type": "script"
                }
              ],
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "label": "Tag Provider",
            "placeHolder": "Select Asset Type"
          },
          "path": "Lib/P\u0026ID/Framework/Dropbox",
          "style": {
            "z-index": 99999999999999
          }
        },
        "type": "ia.display.view"
      },
      {
        "events": {
          "component": {
            "onNodeClick": {
              "config": {
                "script": "\tself.view.custom.tagPath \u003d event.path"
              },
              "scope": "G",
              "type": "script"
            }
          }
        },
        "meta": {
          "name": "TagBrowseTree"
        },
        "position": {
          "grow": 1
        },
        "propConfig": {
          "props.root.path": {
            "binding": {
              "config": {
                "expression": "coalesce({../Providers.props.params.value}, \"\")"
              },
              "type": "expr"
            }
          }
        },
        "props": {
          "root": {},
          "selection": {
            "mode": "single"
          },
          "style": {
            "backgroundColor": "#B8B8B800",
            "border": "none",
            "color": "--color-label-0",
            "fontSize": 14,
            "overflow": "auto"
          }
        },
        "type": "ia.display.tag-browse-tree"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Icon"
            },
            "position": {
              "basis": "20px",
              "shrink": 0
            },
            "props": {
              "color": "--color-label-2",
              "path": "nader/tag-browse",
              "style": {
                "margin-right": "5px"
              }
            },
            "type": "ia.display.icon"
          },
          {
            "meta": {
              "name": "Path"
            },
            "position": {
              "grow": 1
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "path": "view.custom.tagPath"
                  },
                  "type": "property"
                }
              }
            },
            "props": {
              "textStyle": {
                "color": "--color-label-2",
                "fontSize": 12
              }
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Result"
        },
        "position": {
          "shrink": 0
        },
        "props": {
          "alignItems": "center",
          "style": {
            "margin-bottom": "5px"
          }
        },
        "type": "ia.container.flex"
      },
      {
        "children": [
          {
            "events": {
              "component": {
                "onActionPerformed": {
                  "config": {
                    "draggable": false,
                    "id": "tag-broswer-popup",
                    "modal": false,
                    "overlayDismiss": false,
                    "resizable": false,
                    "showCloseIcon": false,
                    "type": "close",
                    "viewPath": "Lib/App/Tag Browser/Poupup",
                    "viewportBound": false
                  },
                  "scope": "C",
                  "type": "popup"
                }
              }
            },
            "meta": {
              "name": "Cancel"
            },
            "position": {
              "basis": "150px"
            },
            "props": {
              "image": {
                "icon": {
                  "path": "material/cancel"
                }
              },
              "primary": false,
              "style": {
                "border": "solid 2px var(--callToAction)",
                "fd": "value",
                "margin-right": 10
              },
              "text": "Cancel"
            },
            "type": "ia.input.button"
          },
          {
            "events": {
              "dom": {
                "onClick": {
                  "config": {
                    "script": "\tsystem.perspective.sendMessage(\"tag-browser-selected\", self.view.custom.tagPath, scope \u003d \"page\")\n\tsystem.perspective.closePopup(\"tag-broswer-popup\")"
                  },
                  "scope": "G",
                  "type": "script"
                }
              }
            },
            "meta": {
              "name": "Button"
            },
            "position": {
              "basis": "150px"
            },
            "props": {
              "text": "SELECT"
            },
            "type": "ia.input.button"
          }
        ],
        "meta": {
          "name": "FlexContainer"
        },
        "position": {
          "basis": "34px",
          "shrink": 0
        },
        "props": {
          "justify": "center",
          "style": {
            "overflow": "visible"
          }
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "dashboard-card",
        "padding": "10px"
      }
    },
    "type": "ia.container.flex"
  }
}