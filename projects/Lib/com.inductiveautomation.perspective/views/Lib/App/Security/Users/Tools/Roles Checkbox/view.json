{
  "custom": {},
  "params": {
    "role": "admin",
    "selected": false
  },
  "propConfig": {
    "params.role": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.selected": {
      "paramDirection": "inout",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 35,
      "width": 198
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Checkbox"
        },
        "position": {
          "shrink": 0
        },
        "propConfig": {
          "props.selected": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "view.params.selected"
              },
              "type": "property"
            }
          },
          "props.text": {
            "binding": {
              "config": {
                "path": "view.params.role"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "style": {
            "classes": "form-input3_label",
            "height": 32
          }
        },
        "type": "ia.input.checkbox"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column",
      "style": {
        "classes": "UserManagement/MainFont",
        "overflow": "visible"
      }
    },
    "type": "ia.container.flex"
  }
}