{
  "custom": {
    "currentView": "home"
  },
  "params": {
    "userSource": "default"
  },
  "permissions": {
    "securityLevels": [
      {
        "children": [
          {
            "children": [
              {
                "children": [],
                "name": "Administrator"
              }
            ],
            "name": "Roles"
          }
        ],
        "name": "Authenticated"
      }
    ],
    "type": "AllOf"
  },
  "propConfig": {
    "custom.currentView": {
      "persistent": true
    },
    "params.userSource": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 600,
      "width": 527
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Home"
        },
        "position": {
          "basis": "320px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "{view.custom.currentView} \u003d \"home\""
              },
              "type": "expr"
            }
          },
          "props.params.userSource": {
            "binding": {
              "config": {
                "path": "view.params.userSource"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "params": {
            "mode": "add",
            "selectedRole": "",
            "selectedUser": ""
          },
          "path": "Lib/App/Security/Users/Home",
          "style": {
            "classes": "Animations/delay-display"
          }
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "AddEditUser"
        },
        "position": {
          "basis": "320px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "{view.custom.currentView} \u003d \"addEditUser\""
              },
              "type": "expr"
            }
          },
          "props.params.mode": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "../Home.props.params.mode"
              },
              "type": "property"
            }
          },
          "props.params.selectedUser": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "../Home.props.params.selectedUser"
              },
              "type": "property"
            }
          },
          "props.params.userSource": {
            "binding": {
              "config": {
                "path": "view.params.userSource"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "path": "Lib/App/Security/Users/AddEditUser",
          "style": {
            "classes": "Animations/delay-display"
          }
        },
        "type": "ia.display.view"
      },
      {
        "meta": {
          "name": "AddEditRoles"
        },
        "position": {
          "basis": "320px",
          "grow": 1
        },
        "propConfig": {
          "position.display": {
            "binding": {
              "config": {
                "expression": "{view.custom.currentView} \u003d \"addEditRoles\""
              },
              "type": "expr"
            }
          },
          "props.params.mode": {
            "binding": {
              "config": {
                "path": "../Home.props.params.mode"
              },
              "type": "property"
            }
          },
          "props.params.selectedRole": {
            "binding": {
              "config": {
                "path": "../Home.props.params.selectedRole"
              },
              "type": "property"
            }
          },
          "props.params.selectedUser": {
            "binding": {
              "config": {
                "bidirectional": true,
                "path": "../Home.props.params.selectedUser"
              },
              "type": "property"
            }
          },
          "props.params.userSource": {
            "binding": {
              "config": {
                "path": "view.params.userSource"
              },
              "type": "property"
            }
          }
        },
        "props": {
          "path": "Lib/App/Security/Users/AddEditRoles",
          "style": {
            "classes": "Animations/delay-display"
          }
        },
        "type": "ia.display.view"
      }
    ],
    "meta": {
      "name": "root"
    },
    "props": {
      "direction": "column"
    },
    "scripts": {
      "customMethods": [],
      "extensionFunctions": null,
      "messageHandlers": [
        {
          "messageType": "UserManagement_ChangeView",
          "pageScope": true,
          "script": "\t# Update current view and refresh tables.\n\tself.view.custom.currentView \u003d payload[\u0027view\u0027]\n\tsystem.perspective.sendMessage(\"UserManagement_RefreshTables\", scope\u003d\"page\")",
          "sessionScope": false,
          "viewScope": false
        }
      ]
    },
    "type": "ia.container.flex"
  }
}