                "onChange": {
                  "enabled": null,
                  "script": "\tif self.getSibling(\"dock\").custom.show:\n\t\tself.getSibling(\"dock\").custom.show \u003d False\t\n\tif self.view.custom.dashboardMode:\n\t\t\t\t\n\t\tif self.props.path.find(\"/Dashboard/\") \u003d\u003d -1:\n\t\t\tview \u003d self.props.path.split(\"/\")[-1]\n\t\t\tdahboardView \u003d self.props.path.replace(view, \"\") + \"Dashboard/\" + view\t\t\t\n\t\t\tself.props.path \u003d self.props.path.replace(view, \"\") + \"Dashboard/\" + view\n\t\t\tself.custom.mobileMode \u003d True\n\t\t\tPID.closePopups(self)\n\telse:\n\t\tif self.props.path.find(\"/Dashboard/\") !\u003d -1:\n\t\t\tself.props.path \u003d self.props.path.replace(\"/Dashboard\", \"\")\n\t\t\tself.custom.mobileMode \u003d False\t\t\t\n\t\n\t\n\t\n\tif not self.view.custom.dashboardMode:\n\t\t\n\t\tview \u003d str(self.view.id).split(\u0027@\u0027)[0]\n\t\titem \u003d {\"view\": view, \"subView\": currentValue.value}\n\t\t\n\t\ti \u003d 0\n\t\tfor openedView in self.session.custom[\u0027openViews\u0027]:\n\t\t\tif openedView[\"view\"] \u003d\u003d item[\"view\"]:\n\t\t\t\tif openedView[\"subView\"] \u003d\u003d item[\"subView\"]:\n\t\t\t\t\treturn\n\t\t\t\t\n\t\t\t\tif openedView[\"subView\"] \u003d\u003d \"\":\n\t\t\t\t\tself.session.custom[\u0027openViews\u0027].pop(i)\n\t\t\t\t\tbreak\n\t\t\ti \u003d i + 1\n\t\t\n\t\tself.session.custom[\u0027openViews\u0027].append({\"view\": view, \"subView\": currentValue.value})"
                }
              },
              "props.style.zoom": {
                "binding": {
                  "config": {
                    "struct": {
                      "dashboard": "{view.custom.dashboardMode}",
                      "height": "if({page.props.dimensions.primaryView.height} \u003d 0 , {view.props.defaultSize.height}, {page.props.dimensions.primaryView.height})",
                      "mobileMode": "{view.custom.mobileMode}",
