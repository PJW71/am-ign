{
  "custom": {},
  "params": {
    "options": [
      {
        "label": "Per Hour",
        "value": 0
      },
      {
        "label": "Per Day",
        "value": 1
      },
      {
        "label": "Per Flow Period",
        "value": 2
      }
    ],
    "tagPath": "[NOVO]Eddy Co/Astrodog S CTB/Flare/TMM100/MSU/CVO"
  },
  "propConfig": {
    "params.options": {
      "paramDirection": "input",
      "persistent": true
    },
    "params.tagPath": {
      "paramDirection": "input",
      "persistent": true
    }
  },
  "props": {
    "defaultSize": {
      "height": 44,
      "width": 162
    },
    "dropConfig": {
      "dataTypes": [
        {
          "action": "path",
          "param": "tagPath",
          "type": "Float4"
        },
        {
          "action": "path",
          "param": "tagPath",
          "type": "Float8"
        },
        {
          "action": "path",
          "param": "tagPath",
          "type": "Int4"
        },
        {
          "action": "path",
          "param": "tagPath",
          "type": "Int8"
        }
      ]
    }
  },
  "root": {
    "children": [
      {
        "meta": {
          "name": "Dropdown"
        },
        "position": {
          "height": 34,
          "width": "100%",
          "y": 10
        },
        "propConfig": {
          "props.options": {
            "binding": {
              "config": {
                "path": "view.params.options"
              },
              "overlayOptOut": true,
              "type": "property"
            }
          },
          "props.value": {
            "binding": {
              "config": {
                "bidirectional": true,
                "fallbackDelay": 2.5,
                "mode": "indirect",
                "references": {
                  "tagPath": "{view.params.tagPath}"
                },
                "tagPath": "{tagPath}"
              },
              "overlayOptOut": true,
              "type": "tag"
            }
          }
        },
        "props": {
          "style": {
            "classes": "pid-input",
            "fontSize": 14
          }
        },
        "type": "ia.input.dropdown"
      },
      {
        "children": [
          {
            "meta": {
              "name": "Label"
            },
            "position": {
              "shrink": 0
            },
            "propConfig": {
              "props.text": {
                "binding": {
                  "config": {
                    "fallbackDelay": 2.5,
                    "mode": "indirect",
                    "references": {
                      "tagPath": "{view.params.tagPath}"
                    },
                    "tagPath": "{tagPath}.Documentation"
                  },
                  "overlayOptOut": true,
                  "type": "tag"
                }
              }
            },
            "props": {
              "style": {
                "backgroundColor": "--color-pid-popup",
                "margin-left": 8,
                "padding": 3
              },
              "textStyle": {
                "classes": "pid-label"
              }
            },
            "type": "ia.display.label"
          },
          {
            "meta": {
              "name": "Dummy"
            },
            "position": {
              "grow": 1
            },
            "type": "ia.display.label"
          }
        ],
        "meta": {
          "name": "Label"
        },
        "position": {
          "height": 16,
          "width": "100%",
          "y": 2.5
        },
        "type": "ia.container.flex"
      }
    ],
    "meta": {
      "name": "root"
    },
    "type": "ia.container.coord"
  }
}